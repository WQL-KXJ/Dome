<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloudalibaba.dao.orderDao">

    <insert id="insert_order">
        insert into order (user_id,product_id,total_amount,status) values (#{userId},#{productId},#{totalAmount},#{status})
    </insert>
</mapper>